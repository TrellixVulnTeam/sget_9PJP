import { Pipe } from '@angular/core';
/**
 *
 * Cria PIPE de CEP para valores.
 *
 * Usage:
 * {{ value | cep }}
*/
export class CepPipe {
    transform(value) {
        if (value === null) {
            return '-';
        }
        else {
            value = value.replace(/[^0-9A-Za-z]/g, '');
            return value.substring(0, 5) + '-' + value.substring(5, 8);
        }
    }
}
CepPipe.decorators = [
    { type: Pipe, args: [{
                name: 'cep'
            },] }
];
